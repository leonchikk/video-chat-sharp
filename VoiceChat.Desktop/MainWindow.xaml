<Window x:Class="VoiceChat.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoiceChat.Desktop"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="MadJuns" Height="200" Width="244">
    <Grid Margin="0,0,10,0">
        <Button Click="MicroOnButton_Click" x:Name="MicroOnButton" Content="Micro On" Margin="28,20,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75"/>
        <Button Click="MicroOffButton_Click"  x:Name="MicroOffButton" Content="Micro Off" HorizontalAlignment="Left" Margin="116,20,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
