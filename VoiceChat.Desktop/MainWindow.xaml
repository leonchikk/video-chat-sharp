<Window x:Class="VoiceChat.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoiceChat.Desktop"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Click="MicroOnButton_Click" x:Name="MicroOnButton" Content="Micro On" HorizontalAlignment="Left" Margin="611,17,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Click="MicroOffButton_Click"  x:Name="MicroOffButton" Content="Micro Off" HorizontalAlignment="Left" Margin="699,17,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Name="FPS" HorizontalAlignment="Left" Margin="729,17,0,0" VerticalAlignment="Top" Height="33" Width="45" Foreground="Black"/>
        <Label x:Name="FPS_Incoming" HorizontalAlignment="Left" Margin="291,102,0,0" VerticalAlignment="Top" Height="33" Width="45" Foreground="Black"/>
    </Grid>
</Window>
